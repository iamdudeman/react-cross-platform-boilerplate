  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "dev": "node node_modules/electron-forge/dist/forge.js start",
    "prod": "node node_modules/electron-forge/dist/forge.js make",
    "test": "../node_modules/mocha/bin/mocha ./src/**/*.test.js --compilers js:babel-core/register"
  },
  "keywords": [],
  "author": "timothysolum",
  "license": "MIT",
  "config": {
    "forge": {
